DAEMON_NAME="zenpython"

. $ZENHOME/bin/zenfunctions

MYPATH=`python -c "import os.path; print os.path.realpath('$0')"`
THISDIR=`dirname $MYPATH`
PRGHOME=`dirname $THISDIR`
PRGNAME=zenpython.py
CFGFILE=$CFGDIR/zenpython.conf

generic "$@"
